USE NICATRAV
GO
CREATE SCHEMA BD_REF
GO
CREATE TABLE BD_REF.PERSONA
(
    ID INT IDENTITY(1,1) PRIMARY KEY NOT NULL,
    NOMBRE NVARCHAR(MAX),
    CEDULA CHAR(16),
    LOTE INT
)
CREATE TABLE BD_REF.TELEFONO
(
    TELEFONO CHAR(8) PRIMARY KEY NOT NULL,
    IDPERSONA INT FOREIGN KEY REFERENCES BD_REF.PERSONA(ID),
    CALLED BIT DEFAULT 0,
    DATECALL DATE,
    LOTE INT
)






